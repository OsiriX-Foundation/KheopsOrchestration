					"listen": "prerequest",
					"script": {
						"id": "f20f2c04-9f39-4f84-8a01-0d8770a7ecc4",
						"type": "text/javascript",
						"exec": [
							"function wait(ms)",
							"{",
							"    var d = new Date();",
							"    var d2 = null;",
							"    do { d2 = new Date(); }",
							"    while(d2-d < ms);",
							"}",
							"",
							"wait(15000);"
						]
					}
				}
